                        android:layout_height="wrap_content"
                        android:background="@drawable/round_upper_border"
                        android:orientation="horizontal">


                        <ImageView
                            android:id="@+id/imageView1"
                            android:layout_width="180dp"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:background="@drawable/round_upper_border"
                            android:contentDescription="@string/footwear"
                            app:srcCompat="@drawable/s1" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/round_lower_border"
                        android:baselineAligned="false"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <Space
                            android:layout_width="5dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">


                            <TextView
                                android:id="@+id/textView1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical|start"
                                android:text="@string/AMLFit"
                                android:textColor="#686060" />

                            <TextView
                                android:id="@+id/textView11"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center_vertical|start"
                                android:singleLine="false"
                                android:text="@string/AMLFit"
                                android:textColor="#000000"
                                android:textSize="20sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/textViewCart1"
                                android:layout_width="45dp"
                                android:layout_height="45dp"
                                android:background="@drawable/baseline_shopping_bag_24" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <Space
